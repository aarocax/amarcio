<!DOCTYPE html>
<html>

  <head>
    <meta charset="UTF-8" />
    <title>{% block title %}Fundación Andrés Marcio{% endblock %}</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <link rel="apple-touch-icon" href="{{ asset('favicon.ico') }}" />
    <link rel="stylesheet" href="{{ asset('css/styles.css') }}">
    <link rel="stylesheet" href="{{ asset('css/selectric.css') }}">
    <link rel="stylesheet" href="{{ asset('css/remodal.css') }}">
    <link rel="stylesheet" href="{{ asset('css/remodal-default-theme.css') }}">
    <link rel="stylesheet" href="{{ asset('css/unslider.css') }}">
    {% block stylesheets %}{% endblock %}
  </head>

  <body>
  <div class="remodal-bg">


    {% block header %}{% endblock %}

    <main>
      <article>
        {% block body %}{% endblock %}
      </article>
    </main>

    {% block footer %}{% endblock %}

    <script src="{{ asset('js/jquery-2.2.3.min.js') }}"></script>
    <script src="{{ asset('js/scripts.js') }}"></script>
    <script src="{{ asset('js/remodal.js') }}"></script>
    <script src="{{ asset('js/jquery.selectric.min.js') }}"></script>
    <script src="{{ asset('js/unslider.js') }}"></script>
    <script src="https://cdn.jsdelivr.net/npm/jquery-validation@1.17.0/dist/jquery.validate.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/jquery-validation@1.17.0/dist/additional-methods.min.js"></script>
    <script src='https://www.google.com/recaptcha/api.js'></script>
    <script src="{{ asset('js/form-validation.js') }}"></script>
    {% block javascript %}{% endblock %}
  </div>
  </body>
  {{ include('amarcio/privacy-policy.html.twig') }}
</html>
